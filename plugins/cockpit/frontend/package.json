{
  "name": "camunda-cockpit-sample-plugin",
  "version": "0.0.1",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "build:watch": "NODE_ENV=development rollup -c -w",
    "copy": "cpx2 \"dist/**/*\" \"../target/classes/plugin-webapp/migrator-plugin/app/\"",
    "watch": "npm run build:watch & chokidar \"dist/**/*\" -c \"npm run copy\"",
    "livereload": "node livereload-server.js",
    "dev": "NODE_ENV=development rollup -c -w & npm run livereload",
    "dev:full": "NODE_ENV=development npm-run-all --parallel build:watch livereload"
  },
  "type": "module",
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "react": "~18.3.1",
    "react-dom": "~18.3.1"
  },
  "devDependencies": {
    "@babel/core": "~7.25.2",
    "@babel/plugin-transform-runtime": "~7.25.4",
    "@babel/preset-env": "~7.25.4",
    "@babel/preset-react": "~7.24.7",
    "@rollup/plugin-babel": "~6.0.4",
    "@rollup/plugin-commonjs": "~28.0.0",
    "@rollup/plugin-node-resolve": "~15.3.0",
    "@rollup/plugin-replace": "~6.0.1",
    "chokidar-cli": "^3.0.0",
    "cpx2": "^8.0.0",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5",
    "rollup": "~4.23.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "~4.0.0",
    "sass": "^1.79.4"
  }
}
